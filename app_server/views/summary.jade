extends layout

block content
  .container
    .row
      .col-xs-12.col-md-6.col-md-offset-3
        h1.page-header= title
        if cant_items == 0
          h3 Ningun archivo para procesar.
        else if cant_items == 1
          h3 Fue procesado un archivo.
        else
          h3 Fueron procesados #{cant_items} archivos.
        p
          a(href="/") Volver
